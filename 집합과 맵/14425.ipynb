    "N, M = map(int, input().split())\n",
    "\n",
    "s = set(input().rstrip() for _ in range(N))\n",
    "\n",
    "cnt = 0\n",
    "for _ in range(M):\n",
    "    if input().rstrip() in s:\n",
    "        cnt += 1\n",
    "\n",
    "print(cnt)"
