    "dial = ['ABC', 'DEF', 'GHI', 'JKL', 'NMO', 'PQRS', 'TUV', 'WXYZ']\n",
    "word = input()\n",
    "time = 0\n",
    "\n",
    "for i in range(len(word)):\n",
    "    for j in dial:\n",
    "        if word[i] in j:\n",
    "            time += dial.index(j) + 3\n",
    "print(time)\n"
