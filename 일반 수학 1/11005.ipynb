    "a ,b = map(int, input().split())\n",
    "ans = \"\"\n",
    "while a != 0:\n",
    "    n = a % b\n",
    "    if n >= 10:\n",
    "        ans += chr(n+55)\n",
    "    else:\n",
    "        ans += str(n)\n",
    "    a //= b\n",
    "print(ans[::-1])"
