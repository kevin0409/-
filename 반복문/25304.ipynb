    "total = int(input())\n",
    "index = int(input())\n",
    "price = []\n",
    "\n",
    "for i in range(index):\n",
    "    price_i, index_i = map(int,input().split())\n",
    "    price.append(price_i * index_i)\n",
    "    \n",
    "total_i = sum(price)\n",
    "\n",
    "if total_i == total:\n",
    "    print(\"Yes\")\n",
    "else:\n",
    "    print(\"No\")\n",
    "    "
