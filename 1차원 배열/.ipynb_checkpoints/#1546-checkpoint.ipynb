    "n = int(input())\n",
    "score = list(map(int, input().split()))\n",
    "top = max(score)\n",
    "\n",
    "n_score = []\n",
    "for i in range(n):\n",
    "    n_score.append(score[i]/top*100)\n",
    "print(sum(n_score)/n)"
