    "n, m = map(int, input().split())\n",
    "n_list = [0] * (n+1)\n",
    "for _ in range(m):\n",
    "    i, j, k = map(int, input().split())\n",
    "    for r in range(i, j+1):\n",
    "        n_list[r] = k\n",
    "for i in range(1, n+1):\n",
    "    print(n_list[i], end = ' ')"
   ]
