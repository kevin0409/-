    "n_list = list(map(int, input().split()))\n",
    "\n",
    "top = n_list[0]\n",
    "bot = n_list[0]\n",
    "\n",
    "for i in range(n):\n",
    "    if n_list[i] > top:\n",
    "        top = n_list[i]\n",
    "    if n_list[i] < bot:\n",
    "        bot = n_list[i]\n",
    "print(bot, top)"
