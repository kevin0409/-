   "source": [
    "case = int(input())\n",
    "\n",
    "for _ in range(case):\n",
    "    s_list = list(map(int, input().split()))\n",
    "    avg = sum(s_list[1:])/s_list[0]\n",
    "    count = 0\n",
    "    for i in s_list[1:]:\n",
    "        if i > avg:\n",
    "            count += 1\n",
    "    p = (count/s_list[0]) * 100\n",
    "    print('{:.3f}%'.format(p))\n"
   ]
